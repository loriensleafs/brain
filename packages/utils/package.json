{
	"name": "@brain/utils",
	"version": "1.0.0",
	"type": "module",
	"main": "src/index.ts",
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"build": "bun build src/index.ts --outdir dist --target node",
		"typecheck": "tsc --noEmit",
		"test": "bun run --bun vitest run",
		"test:watch": "bun run --bun vitest",
		"test:coverage": "bun run --bun vitest run --coverage",
		"clean": "rm -rf dist"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/node": "^25.0.8",
		"bun-types": "^1.3.8",
		"typescript": "^5.7.0",
		"vitest": "^4.0.18"
	},
	"engines": {
		"bun": ">=1.3.8"
	}
}
